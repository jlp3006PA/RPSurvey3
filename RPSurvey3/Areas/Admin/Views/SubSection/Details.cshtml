@model RPSurvey3.Models.SubSection

@{
  ViewData["Title"] = "Details of SubSections";
  Layout = "~/Views/Shared/_Layout.cshtml";
}
<br />
<h2 class="text-primary">SubSection Details</h2>
<br />

<div class="backgroundWhite rounded">
  <fieldset disabled>

    <div class="form-group row">
      <div class="col-2">
        <label asp-for="Name" class="custom-label"></label>
      </div>
      <div class="col-5">
        <input asp-for="Name" class="form-control" />
      </div>
    </div>
    <div class="form-group row">
      <div class="col-2">
        <label asp-for="Section.Name" class="custom-label"></label>
      </div>
      <div class="col-5">
        <input asp-for="Section.Name" class="form-control" />
      </div>
    </div>

    @* Temp:  This is for the Createdby *@
    <div class="form-group row">
      <div class="col-2">
        <label asp-for="CreatedBy" class="col-form-label"></label>
      </div>
      <div class="col-5">
        <input asp-for="CreatedBy" class="form-control" />
      </div>
      <span asp-validation-for="CreatedBy" class="text-danger"></span>
    </div>

    @* Temp:  This is for the CreatedTime *@

    <div class="form-group row">

      <div class="col-2">
        <label asp-for="DateCreated" class="col-form-label"></label>
      </div>

      <div class="col-5">
        <input asp-for="DateCreated" class="form-control" />
      </div>

      <span asp-validation-for="DateCreated" class="text-danger"></span>
    </div>

    <div class="form-group row">

      <div class="col-2">
        <label asp-for="DisplayOrder" class="col-form-label"></label>
      </div>

      <div class="col-5">
        <input asp-for="DisplayOrder" class="form-control" />
      </div>

      <span asp-validation-for="DisplayOrder" class="text-danger"></span>
    </div>

    <div class="form-group row">

      <div class="col-5 offset-2">
        <partial name="_DetailsBackToList" model="Model.Id" />
      </div>
    </div>

  </fieldset>



</div>