@model RPSurvey3.Models.ViewModels.QuestionViewModel

<br />
<h2 class="text-primary">Sub Section Delete</h2>
<br />
<form method="post">
  <div class="border backgroundWhite">
    <input type="hidden" asp-for="Question.Id" />

    <fieldset disabled>
      <div class="form-group row">
        <div class="col-2">
          <label asp-for="Question.SurveyQuestion" class="custom-label; text-primary"></label>
        </div>
        <div class="col-5">
          <input asp-for="Question.SurveyQuestion" class="form-control" />
        </div>
      </div>

      <div class="form-group row">
        <div class="col-2">
          <label asp-for="Question.Section.Name" class="custom-label; text-primary"></label>
        </div>
        <div class="col-5">
          <input asp-for="Question.Section.Name" class="form-control" />
        </div>
      </div>

      <div class="form-group row">
        <div class="col-2">
          <label asp-for="Question.SubSection.Name" class="custom-label; text-primary"></label>
        </div>
        <div class="col-5">
          <input asp-for="Question.SubSection.Name" class="form-control" />
        </div>
      </div>

      <div class="form-group row">
        <div class="col-2">
          <label asp-for="Question.DisplayOrder" class="col-form-label; text-primary"></label>
        </div>
        <div class="col-2">
          <input asp-for="Question.DisplayOrder" class="form-control" />
        </div>
      </div>

    </fieldset>
    <br />
    <div class="form-group row">
      <div class="col-5 offset-2">
        <partial name="_DeleteAndBackToListButton" model="Model.Question.Id" />
      </div>
    </div>
  </div>
</form>

